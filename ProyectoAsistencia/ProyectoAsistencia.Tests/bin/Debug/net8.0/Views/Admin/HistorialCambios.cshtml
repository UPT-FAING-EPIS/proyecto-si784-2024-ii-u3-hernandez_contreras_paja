@model IEnumerable<ProyectoAsistencia.Models.Historial>

<h2>Historial de Cambios</h2>
<table class="table mt-3">
    <thead>
        <tr>
            <th>ID</th>
            <th>Acción</th>
            <th>Fecha</th>
            <th>Usuario</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var cambio in Model)
        {
            <tr>
                <td>@cambio.Id</td>
                <td>@cambio.Accion</td>
                <td>@cambio.Fecha</td>
                <td>@cambio.Usuario</td>
            </tr>
        }
    </tbody>
</table>
